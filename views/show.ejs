            <div class="reviewcard">
                <div class="heading">
                    <h2>Leave a Review</h2>
                </div>
                <form id="reviewForm" action="/listings/<%= listing.id %>/reviews" method="POST">
                    <div class="comment">
                        <p>Comment</p>
                        <textarea id="commentInput" name="comment" cols="50" rows="7" class="textarea"></textarea>
                    </div>
                    <div class="rating">
                        <p>Rating</p>
                        <input type="range" min = "1" max = "5" name="rating">
                    </div>
                    <button onclick="validateForm()">Add Review</button>
